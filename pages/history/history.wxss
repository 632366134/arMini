.container {
  width: 100%;
  height: 100vh;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.container .swiper {
  margin-top: 210rpx;
  width: 100%;
  height: 100%;
  display: flex;
}
.container .swiper .swiper-item-view {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.container .swiper .swiper-item-view .addtime {
  color: #fff;
  z-index: 99;
}
.container .swiper .swiper-item-view .swiper-item {
  justify-content: center;
  width: 613rpx;
  height: 923rpx;
  display: flex;
  flex-direction: column;
  background-size: 100% 100%;
  align-items: center;
  background-image: url("data:image/png;base64,");
}
.container .swiper .swiper-item-view .swiper-item .scrollImage {
  margin-top: 54rpx;
  border-radius: 20rpx;
  width: 500rpx;
  height: 700rpx;
}
.container .swiper .swiper-item-view .swiper-item text {
  margin-top: 25rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #020202;
  line-height: 48rpx;
}
